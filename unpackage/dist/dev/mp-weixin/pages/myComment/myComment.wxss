.my-comment {
		width: 100vw;
		height: 100vh;
		overflow-y: scroll;
		transition: all 3s ease;
}
.box {
		width: 100vw;
		display: flex;
		flex-direction: row;
		margin-bottom: 5px;
}
.right-bar {
		width: 8vw;
		background-color: white;
		display: flex;
		align-items: center;
		justify-content: center;
}
.left-bar {
		width: 92vw;
}
.icon {
		font-size: 15px;
		color: #ccc;
}
.hint {
		position: fixed;
		top: 0;
		width: 100%;
		background-color: white;
		height: 30px;
		display: flex;
		align-items: center;
		justify-content: center;
		font-size: 12px;
		color: #ccc;
}
.uni-swipe {
		margin-bottom: 10px;
}
.slot {
		width: 20vw;
		display: flex;
		flex-direction: column;
		height: 100%;
		background-color: white;
}
.slot-button {
		height: 50%;
		display: flex;
		flex-direction: column;
		justify-content: center;
		align-items: center;
		color: #777;
}
