.icon {
  font-size: 45rpx;
  font-weight: 300;
}
.act_detail swiper {
  height: 500rpx;
}
.act_detail swiper image {
  width: 100%;
  height: 100%;
}
.act_detail .basic {
  padding: 10px;
}
.act_detail .basic .price {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 30rpx;
  color: darkred;
}
.act_detail .basic .price text:nth-child(3) {
  color: #ccc;
  font-size: 28rpx;
  text-decoration: line-through;
  margin-left: 20rpx;
}
.act_detail .basic .price .new_price {
  line-height: 30rpx;
  font-size: 30rpx;
}
.act_detail .basic .act_name {
  font-size: 32rpx;
  line-height: 60rpx;
  font-weight: 600;
}
.act_detail .blank_line {
  height: 10rpx;
  width: 100%;
  background: #eee;
}
.act_detail .number {
  padding: 0 10px;
  font-size: 32rpx;
  line-height: 50rpx;
}
.act_detail .description .tit {
  font-size: 32rpx;
  padding-left: 10px;
  border-bottom: 2px solid #eee;
  line-height: 70rpx;
}
.act_detail .description .scroll_page {
  padding: 10px;
  width: calc(100% - 20px);
}
.act_detail .act_buy {
  display: flex;
  flex-direction: column;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
}